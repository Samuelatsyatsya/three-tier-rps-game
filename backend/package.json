{
  "name": "backend",
  "version": "1.0.0",
  "description": "Rock Paper Scissors Game Backend",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:migrate": "node -e \"import('./src/config/database.js').then(({ sequelize }) => sequelize.sync({ alter: true }))\"",
    "db:reset": "node -e \"import('./src/config/database.js').then(({ sequelize }) => sequelize.sync({ force: true }))\""
  },
  "dependencies": {
    "aws-sdk": "^2.1693.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.0",
    "sequelize": "^6.32.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
